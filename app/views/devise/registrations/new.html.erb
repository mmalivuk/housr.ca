<h2>Realtor Sign up</h2>
<br>
<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.label :email %>&nbsp
  <%= f.email_field :email %></div><br>

  <div><%= f.label :password %>&nbsp
  <%= f.password_field :password %></div><br>

  <div><%= f.label :password_confirmation, "Confirm Password" %>&nbsp
  <%= f.password_field :password_confirmation %></div><br>
<br>
  <div><%= f.submit "Sign up" %></div>
<% end %>

<%= render :partial => "devise/shared/links" %>
